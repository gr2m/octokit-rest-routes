{
  "name": "@octokit/routes",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public",
    "tag": "beta"
  },
  "description": "Machine-readable, always up-to-date GitHub REST API route specifications",
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "main": "lib/index.js",
  "scripts": {
    "test": "tap --coverage --timeout 60 'test/**/*-test.js'",
    "test:ci": "node bin/run-tests.js",
    "update": "node bin/octokit-rest-routes.js update",
    "check": "node bin/octokit-rest-routes.js check",
    "routes:lint": "speccy lint openapi/api.github.com/index.json --skip openapi-tags",
    "routes:server": "speccy serve openapi/api.github.com/index.json",
    "routes:regenerate": "npm run routes:regenerate:gh",
    "routes:regenerate:gh": "npm run check -- --cached",
    "routes:regenerate:ghe": "npm run check -- --cached --ghe",
    "routes:regenerate:all": "npm run routes:regenerate:gh && npm run routes:regenerate:ghe",
    "now-start": "npm run -s routes:server",
    "inspect:routes:regenerate": "node --inspect-brk bin/octokit-rest-routes.js check --cached",
    "inspect:routes:regenerate:ghe": "node --inspect-brk bin/octokit-rest-routes.js check --cached --ghe",
    "inspect:test:endpoints": "tap --node-arg=--inspect-brk --timeout=0 --no-coverage 'test/integration/endpoints-test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octokit/routes.git"
  },
  "keywords": [
    "octokit"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octokit/routes/issues"
  },
  "homepage": "https://github.com/octokit/routes#readme",
  "engines": {
    "node": ">=8.12.0"
  },
  "dependencies": {
    "generate-schema": "^2.6.0",
    "lodash": "^4.17.15",
    "speccy": "^0.11.0",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "execa": "^2.0.0",
    "fs-extra": "^8.0.0",
    "gh-got": "^8.0.0",
    "got": "^9.3.2",
    "javascript-stringify": "^2.0.0",
    "json-diff": "^0.5.3",
    "markdown-table": "^1.1.2",
    "node-fetch": "^2.3.0",
    "normalize-url": "^4.0.0",
    "now": "^16.0.0",
    "pluralize": "^8.0.0",
    "proxyquire": "^2.1.0",
    "semantic-release": "^15.13.21",
    "tap": "^14.5.0",
    "turndown": "^5.0.0",
    "yargs": "^14.0.0"
  },
  "files": [
    "openapi",
    "lib"
  ]
}
