        "type": "string",
        "default": "application/vnd.github.barred-rock-preview+json"
      },
      "required": true
    },
    {
      "name": "owner",
      "in": "path",
      "schema": {
        "type": "string"
      },
      "required": true,
      "description": "owner parameter"
    },
    {
      "name": "repo",
      "in": "path",
      "schema": {
        "type": "string"
      },
      "required": true,
      "description": "repo parameter"
    }
  ],
  "responses": {
