{
  "summary": "Get a single template",
  "description": "The API also allows fetching the source of a single template.\n\nUse the raw [media type](https://developer.github.com/v3/media/) to get the raw contents.\n\n",
  "operationId": "gitignore-get-template",
  "tags": [
    "gitignore"
  ],
  "externalDocs": {
    "description": "API method documentation",
    "url": "https://developer.github.com/v3/gitignore/#get-a-single-template"
  },
  "parameters": [
    {
      "name": "name",
      "in": "path",
      "schema": {
        "type": "string"
      },
      "required": true,
      "description": "name parameter"
    }
  ],
  "x-code-samples": [
    {
      "lang": "Shell",
      "source": "curl \\\n  -H\"Accept: application/vnd.github.v3+json\" \\\n  https://developer.github.com/gitignore/templates/"
    },
    {
      "lang": "JS",
      "source": "octokit.gitignore.get({\n  name: 'name'\n})"
    }
  ],
  "responses": {
    "200": {
      "description": "response",
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "name": {
                "type": "string"
              },
              "source": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
