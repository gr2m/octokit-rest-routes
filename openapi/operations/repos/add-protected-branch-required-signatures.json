      "description": "owner parameter"
    },
    {
      "name": "repo",
      "in": "path",
      "schema": {
        "type": "string"
      },
      "required": true,
      "description": "repo parameter"
    },
    {
      "name": "branch",
      "in": "path",
      "schema": {
        "type": "string"
      },
      "required": true,
      "description": "branch parameter"
    }
  ],
  "x-code-samples": [
