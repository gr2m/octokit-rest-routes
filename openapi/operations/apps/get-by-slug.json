                  },
                  "repos_url": {
                    "type": "string"
                  },
                  "events_url": {
                    "type": "string"
                  },
                  "hooks_url": {
                    "type": "string"
                  },
                  "issues_url": {
                    "type": "string"
                  },
                  "members_url": {
                    "type": "string"
                  },
                  "public_members_url": {
                    "type": "string"
                  },
                  "avatar_url": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                }
              },
              "name": {
                "type": "string"
              },
              "description": {
                "type": "string"
              },
              "external_url": {
