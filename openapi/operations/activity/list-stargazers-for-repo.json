      "source": "curl \\\n  -H\"Accept: application/vnd.github.v3+json\" \\\n  https://developer.github.com/repos/octocat/:repo/stargazers"
    },
    {
      "lang": "JS",
      "source": "octokit.activity.get({\n  owner: 'octocat',\n  repo: 'hello-world'\n})"
    }
