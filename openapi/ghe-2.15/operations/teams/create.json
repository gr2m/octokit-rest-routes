                "type": "number"
              },
              "created_at": {
                "type": "string"
              },
              "updated_at": {
                "type": "string"
              },
              "organization": {
                "type": "object",
                "properties": {
                  "login": {
                    "type": "string"
                  },
                  "id": {
                    "type": "number"
                  },
                  "node_id": {
                    "type": "string"
                  },
                  "url": {
