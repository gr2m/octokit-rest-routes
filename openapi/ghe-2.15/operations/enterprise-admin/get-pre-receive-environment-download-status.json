      "schema": {
        "type": "string",
        "default": "application/vnd.github.v3+json"
      }
    },
    {
      "name": "pre_receive_environment_id",
      "description": "pre_receive_environment_id parameter",
      "in": "path",
      "required": true,
      "schema": {
        "type": "integer"
      }
    },
    {
      "name": "state",
