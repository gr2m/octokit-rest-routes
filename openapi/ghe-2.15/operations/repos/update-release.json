    {
      "lang": "Shell",
      "source": "curl \\\n  -XPATCH \\\n  -H\"Accept: application/vnd.github.v3+json\" \\\n  https://developer.github.com/repos/octocat/hello-world/releases/42 \\\n  -d '{\"tag_name\":\"tag_name\"}'"
    },
    {
      "lang": "JS",
      "source": "octokit.repos.updateRelease({\n  owner: 'octocat',\n  repo: 'hello-world',\n  release_id: 42,\n  tag_name: 'tag_name'\n})"
