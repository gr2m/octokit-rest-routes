{
  "name": "List organization invitation teams",
  "enabledForApps": false,
  "method": "GET",
  "path": "/orgs/:org/invitations/:invitation_id/teams",
  "params": [
    {
      "name": "org",
      "type": "string",
      "required": true,
      "description": "",
      "location": "url"
    },
    {
      "name": "invitation_id",
      "type": "string",
      "required": true,
      "description": "",
      "location": "url"
    },
    {
      "name": "per_page",
      "type": "integer",
      "required": false,
      "description": "Results per page (max 100)",
      "default": 30,
      "location": "query"
    },
    {
      "name": "page",
      "type": "integer",
      "required": false,
      "description": "Page number of the results to fetch.",
      "default": 1,
      "location": "query"
    }
  ],
  "description": "List all teams associated with an invitation. In order to see invitations in an organization, the authenticated user must be an organization owner.",
