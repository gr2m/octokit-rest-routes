{
  "name": "List events for an issue",
  "enabledForApps": true,
  "method": "GET",
  "path": "/repos/:owner/:repo/issues/:number/timeline",
  "params": [
    {
      "name": "owner",
      "type": "string",
      "required": true,
      "description": "",
      "location": "url"
    },
    {
      "name": "repo",
      "type": "string",
      "required": true,
      "description": "",
      "location": "url"
    },
    {
      "name": "number",
      "type": "integer",
      "required": true,
      "description": "",
      "location": "url"
    },
    {
      "name": "per_page",
      "type": "integer",
      "required": false,
      "description": "Results per page (max 100)",
      "default": 30,
      "location": "query"
    },
    {
      "name": "page",
      "type": "integer",
      "required": false,
      "description": "Page number of the results to fetch.",
      "default": 1,
      "location": "query"
    }
  ],
  "description": "",
  "idName": "list-events",
  "documentationUrl": "https://developer.github.com/v3/issues/timeline/#list-events-for-an-issue"
}
