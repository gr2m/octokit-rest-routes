{
  "name": "Update pre-receive hook enforcement",
  "enabledForApps": false,
  "method": "PATCH",
  "path": "/repos/:owner/:repo/pre-receive-hooks/:id",
  "params": [
    {
      "name": "owner",
      "type": "string",
      "required": true,
      "description": ""
    },
    {
      "name": "repo",
      "type": "string",
      "required": true,
      "description": ""
    },
    {
      "name": "id",
      "type": "string",
      "required": true,
      "description": ""
    }
  ],
  "description": "For pre-receive hooks which are allowed to be configured at the repo level, you can set `enforcement`",
  "documentationUrl": "https://developer.github.com/v3/repos/pre_receive_hooks/#update-pre-receive-hook-enforcement"
}
