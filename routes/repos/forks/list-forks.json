{
  "name": "List forks",
  "enabledForApps": true,
  "method": "GET",
  "path": "/repos/:owner/:repo/forks",
  "params": [
    {
      "name": "owner",
      "type": "string",
      "required": true,
      "description": ""
    },
    {
      "name": "repo",
      "type": "string",
      "required": true,
      "description": ""
    },
    {
      "name": "sort",
      "type": "string",
      "description": "The sort order. Can be either `newest`, `oldest`, or `stargazers`.",
      "default": "newest",
      "required": false
    },
    {
      "name": "per_page",
      "type": "number",
      "required": false,
      "description": "Results per page (max 100)",
      "default": 30
    },
    {
      "name": "page",
      "type": "number",
      "required": true,
      "description": "Page number of the results to fetch.",
      "default": 1
    }
  ],
  "description": "",
  "documentationUrl": "https://developer.github.com/v3/repos/forks/#list-forks"
}
