{
  "name": "Create a repository project",
  "enabledForApps": true,
  "method": "POST",
  "path": "/repos/:owner/:repo/projects",
  "params": [
    {
      "name": "owner",
      "type": "string",
      "required": true,
      "description": ""
    },
    {
      "name": "repo",
      "type": "string",
      "required": true,
      "description": ""
    },
    {
      "name": "name",
      "type": "string",
      "description": "The name of the project.",
      "required": true
    },
    {
      "name": "body",
      "type": "string",
      "description": "The body of the project.",
      "required": false
    },
    {
      "name": "per_page",
      "type": "number",
      "required": false,
      "description": "Results per page (max 100)",
      "default": 30
    },
    {
      "name": "page",
      "type": "number",
      "required": true,
      "description": "Page number of the results to fetch.",
      "default": 1
    }
  ],
  "description": "**Note**: The status code may also be `401` or `410`, depending on the scope of the authenticating token.",
  "documentationUrl": "https://developer.github.com/v3/projects/#create-a-repository-project"
}
